FROM ubuntu:16.04
RUN mkdir -p /root/v1.9.11
ADD https://storage.googleapis.com/kubernetes-release/release/v1.9.11/kubernetes.tar.gz /root/v1.9.11/kubernetes.tar.gz
ADD https://storage.googleapis.com/kubernetes-release/release/v1.9.11/kubernetes-client-darwin-amd64.tar.gz /root/v1.9.11/kubernetes-client-darwin-amd64.tar.gz
ADD https://storage.googleapis.com/kubernetes-release/release/v1.9.11/kubernetes-client-linux-amd64.tar.gz /root/v1.9.11/kubernetes-client-linux-amd64.tar.gz

ADD https://storage.googleapis.com/kubernetes-release/release/v1.9.11/kubernetes-server-linux-amd64.tar.gz /root/v1.9.11/kubernetes-server-linux-amd64.tar.gz

ADD  https://storage.googleapis.com/kubernetes-release/release/v1.9.11/kubernetes-node-linux-amd64.tar.gz /root/v1.9.11/kubernetes-node-linux-amd64.tar.gz